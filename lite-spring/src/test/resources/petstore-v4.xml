<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="petStore" class="com.yuanzhipeng.litespring.service.v4.PetStroeService">
        <constructor-arg ref="accountDao"></constructor-arg>
        <constructor-arg ref="itemDao"></constructor-arg>
        <constructor-arg value="1"></constructor-arg>
    </bean>
    <bean id="accountDao" class="com.yuanzhipeng.litespring.dao.v4.AccountDao">
    </bean>
    <bean id="itemDao" class="com.yuanzhipeng.litespring.dao.v4.ItemDao">
    </bean>
</beans>
